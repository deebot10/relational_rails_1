<%= form_with url: "/artists/#{@artist.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :age %>
  <%= form.text_field :age %>

  <%= form.label :genre %>
  <%= form.text_field :genre %>

  <%= form.label :currently_touring %>
  <%= form.text_field :currently_touring %>

  <%= form.submit 'Edit Artist' %>
<% end %>